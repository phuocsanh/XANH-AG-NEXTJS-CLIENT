# Firebase Configuration
# Các giá trị này lấy từ Firebase Console > Project Settings
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id
NEXT_PUBLIC_FIREBASE_VAPID_KEY=your_vapid_key

# Weather API Keys (Ưu tiên lấy từ Firebase Remote Config)
# Nhóm: API_KEY_TOMORROW_IO | Parameters: TOMORROW_API_KEY_1, TOMORROW_API_KEY_2, TOMORROW_API_KEY_3
# Fallback local (chỉ dùng nếu Remote Config trống)
# NEXT_PUBLIC_TOMORROW_API_KEY=key1,key2
